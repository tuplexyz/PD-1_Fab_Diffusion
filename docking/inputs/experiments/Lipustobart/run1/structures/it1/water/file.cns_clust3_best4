module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:Lipustobart_78w.pdb")
evaluate (&filenames.bestfile_2="PREVIT:Lipustobart_91w.pdb")
evaluate (&filenames.bestfile_3="PREVIT:Lipustobart_199w.pdb")
evaluate (&filenames.bestfile_4="PREVIT:Lipustobart_70w.pdb")
 
