module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:Pembrolizumab_54w.pdb")
evaluate (&filenames.bestfile_2="PREVIT:Pembrolizumab_99w.pdb")
evaluate (&filenames.bestfile_3="PREVIT:Pembrolizumab_90w.pdb")
evaluate (&filenames.bestfile_4="PREVIT:Pembrolizumab_162w.pdb")
 
