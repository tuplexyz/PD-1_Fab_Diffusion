module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:Retifanlimab_125w.pdb")
evaluate (&filenames.bestfile_2="PREVIT:Retifanlimab_119w.pdb")
evaluate (&filenames.bestfile_3="PREVIT:Retifanlimab_197w.pdb")
evaluate (&filenames.bestfile_4="PREVIT:Retifanlimab_185w.pdb")
 
