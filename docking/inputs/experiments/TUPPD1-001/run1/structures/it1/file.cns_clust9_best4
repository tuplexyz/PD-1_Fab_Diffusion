module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:TUPPD1-001_3.pdb")
evaluate (&filenames.bestfile_2="PREVIT:TUPPD1-001_62.pdb")
evaluate (&filenames.bestfile_3="PREVIT:TUPPD1-001_117.pdb")
evaluate (&filenames.bestfile_4="PREVIT:TUPPD1-001_80.pdb")
 
