module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:Balstilimab_168w.pdb")
evaluate (&filenames.bestfile_2="PREVIT:Balstilimab_176w.pdb")
evaluate (&filenames.bestfile_3="PREVIT:Balstilimab_173w.pdb")
evaluate (&filenames.bestfile_4="PREVIT:Balstilimab_180w.pdb")
