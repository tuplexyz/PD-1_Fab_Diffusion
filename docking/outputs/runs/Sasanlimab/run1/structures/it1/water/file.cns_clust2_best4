module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:Sasanlimab_117w.pdb")
evaluate (&filenames.bestfile_2="PREVIT:Sasanlimab_6w.pdb")
evaluate (&filenames.bestfile_3="PREVIT:Sasanlimab_28w.pdb")
evaluate (&filenames.bestfile_4="PREVIT:Sasanlimab_21w.pdb")
 
