module(filenames;)
set message off echo off end
evaluate ($count = 1)
while ($count le 2000) loop main
   evaluate (&filenames.bestfile_$count = "")
   evaluate ($count = $count + 1)
end loop main
set message on echo on end
evaluate (&filenames.bestfile_1="PREVIT:TUPPD1-007_4.pdb")
evaluate (&filenames.bestfile_2="PREVIT:TUPPD1-007_34.pdb")
evaluate (&filenames.bestfile_3="PREVIT:TUPPD1-007_25.pdb")
evaluate (&filenames.bestfile_4="PREVIT:TUPPD1-007_38.pdb")
 
